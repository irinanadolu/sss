                                                <fo:table-cell start-indent="0" end-indent="0" padding-bottom="0.3em" padding-left="0.3em" padding-right="0.3em" padding-top="0.3em" display-align="before"><fo:block>perennial</fo:block></fo:table-cell>
                                                <fo:table-cell start-indent="0" end-indent="0" padding-bottom="0.3em" padding-left="0.3em" padding-right="0.3em" padding-top="0.3em" display-align="before"><fo:block>average</fo:block></fo:table-cell>
                                            </fo:table-row>
