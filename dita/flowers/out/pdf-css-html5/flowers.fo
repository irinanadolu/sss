                                                <fo:table-cell start-indent="0" end-indent="0" padding-bottom="0.3em" padding-left="0.3em" padding-right="0.3em" padding-top="0.3em" display-align="before"><fo:block>perennial</fo:block></fo:table-cell>
                                                <fo:table-cell start-indent="0" end-indent="0" padding-bottom="0.3em" padding-left="0.3em" padding-right="0.3em" padding-top="0.3em" display-align="before"><fo:block>acidic</fo:block></fo:table-cell>
                                            </fo:table-row>
                                            <fo:table-row display-align="center">
                                                <fo:table-cell start-indent="0" end-indent="0" padding-bottom="0.3em" padding-left="0.3em" padding-right="0.3em" padding-top="0.3em" display-align="before"><fo:block>Gerbera</fo:block></fo:table-cell>
                                                <fo:table-cell start-indent="0" end-indent="0" padding-bottom="0.3em" padding-left="0.3em" padding-right="0.3em" padding-top="0.3em" display-align="before"><fo:block>annual</fo:block></fo:table-cell>
                                                <fo:table-cell start-indent="0" end-indent="0" padding-bottom="0.3em" padding-left="0.3em" padding-right="0.3em" padding-top="0.3em" display-align="before"><fo:block>sandy, well-drained</fo:block></fo:table-cell>
